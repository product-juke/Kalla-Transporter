<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Cron Job untuk Data Mart Driver Violation -->
        <record id="cron_generate_mart_driver_violation" model="ir.cron">
            <field name="name">Generate Data Mart Driver Violation</field>
            <field name="model_id" ref="model_mart_driver_violation"/>
            <field name="state">code</field>
            <field name="code">model.cron_generate_data_mart()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
            <field name="priority">5</field>
        </record>
        <record id="cron_update_driver_experience_mart" model="ir.cron">
            <field name="name">Generate Data Mart Driver Experience</field>
            <field name="model_id" ref="model_mart_driver_experience"/>
            <field name="state">code</field>
            <field name="code">model._cron_update_data_mart()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
            <field name="priority">6</field>
        </record>
        <record id="cron_generate_mart_status_driver" model="ir.cron">
            <field name="name">Generate Data Mart Driver Status</field>
            <field name="model_id" ref="model_mart_status_driver"/>
            <field name="state">code</field>
            <field name="code">model.cron_generate_data_mart()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="doall">False</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(hours=1)).strftime('%Y-%m-%d %H:00:00')"/>
        </record>
        <record id="cron_sync_vehicle_status_data_mart" model="ir.cron">
            <field name="name">Generate Data Mart Vehicle Status</field>
