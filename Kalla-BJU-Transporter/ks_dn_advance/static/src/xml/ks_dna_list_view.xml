                            <t t-set="count" t-value="count+1"/>
                        </t>
                    </th>
                </t>
                <t t-if="calculation_type == 'custom'">
                    <th class="ks_dn_th"/>
                </t>
            </tr>
        </t>
        <t t-else="">
            <tr style="color:black">

                <t t-foreach="state.list_view_data['label']" t-as="table_header" t-key="table_header_index">
                    <th class="ks_dn_th">
                        <t t-esc="table_header"/>
                    </th>
                </t>
                <t t-if="calculation_type == 'custom'">
                    <th class="ks_dn_th"/>
                </t>
            </tr>
        </t>
    </t>


</templates>