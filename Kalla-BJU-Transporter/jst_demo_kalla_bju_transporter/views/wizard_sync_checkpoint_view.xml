<odoo>
    <record id="view_fleet_last_checkpoint_sync_wizard_form" model="ir.ui.view">
        <field name="name">fleet.last.checkpoint.sync.wizard.form</field>
        <field name="model">fleet.last.checkpoint.sync.wizard</field>
        <field name="arch" type="xml">
            <form string="Sync Data">
                <group>
                    <field name="start_date"/>
                    <field name="end_date"/>
                </group>
                <footer>
                    <button string="Sync" type="object" name="action_sync_data" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
