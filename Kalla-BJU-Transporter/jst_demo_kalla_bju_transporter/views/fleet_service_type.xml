<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="fleet_service_types_view_tree_inherited" model="ir.ui.view">
        <field name="name">fleet.service.types.view.tree.inherited</field>
        <field name="model">fleet.service.type</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_service_types_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree//field[@name='category']" position="after">
                <field name="category_service_id"/>
            </xpath>
        </field>
    </record>

</odoo>
